<h1>Editing season</h1>

<% form_for(@season) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :season_no %><br />
    <%= f.text_field :season_no %>
  </p>
  <p>
    <%= f.label :start_date %><br />
    <%= f.date_select :start_date %>
  </p>
  <p>
    <%= f.label :end_date %><br />
    <%= f.date_select :end_date %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @season %> |
<%= link_to 'Back', seasons_path %>